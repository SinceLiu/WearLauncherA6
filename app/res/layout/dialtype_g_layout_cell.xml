<com.readboy.wearlauncher.view.WatchDialTypeG xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/watch_type_g_black_bg">

    <TextView
        android:id="@+id/date_tvid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dial_g_date_margin_left"
        android:layout_marginTop="@dimen/dial_g_date_margin_top"
        android:textSize="@dimen/dial_g_date_text_size"
        android:textColor="#ffffff"/>

    <!--time-->
    <include
        layout="@layout/digit_clock_g_layout"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginLeft="@dimen/dial_g_clock_margin_left"
        android:layout_marginTop="@dimen/dial_g_clock_margin_top"
        />
</com.readboy.wearlauncher.view.WatchDialTypeG>
